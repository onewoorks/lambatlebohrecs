<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
<meta name="description" content="{{ description }}" />
{% endif %}
{% if keywords %}
<meta name="keywords" content="{{ keywords }}" />
{% endif %}
<script src="catalog/view/javascript/jquery/jquery-2.1.1.min.js" type="text/javascript"></script>
<link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />
<script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,300,700" rel="stylesheet" type="text/css" />
<link href="catalog/view/theme/default/stylesheet/stylesheet.css" rel="stylesheet">
<link href="catalog/view/theme/default/stylesheet/lambatleboh.css?v=1.26" rel="stylesheet">
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}
{% for script in scripts %}
<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}
<script src="catalog/view/javascript/common.js?v=1" type="text/javascript"></script>
{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}
{% for analytic in analytics %}
{{ analytic }}
{% endfor %}
<link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
</head>
<body>

<header id="llb-header" class="container hidden-xs" >
  <div class="row" style="height:80px">
    <div class="col-lg-3 col-sm-3">
      <a href="{{ home }}">
        <img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="logo img-responsive"  />
      </a>
    </div>
    <div class="col-lg-6 col-sm-7 text-center">
        {{ menu }}
    </div>
    <div class="col-lg-3 col-sm-2 text-right">
      <div class="header-line-1">
        <ul class="list-inline">
          <li><a href="https://instagram.com/lambatlebohrecs" target="_blank"><i class="fa fa-instagram" style="font-size:1.8em"  aria-hidden="true"></i></a></li>
        </ul>
      </div>
      <div class="header-line-2">
      
      <ul class="list-inline">
      <li>{{ search }}</li>
        <li class="dropdown"><a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user" style="font-size:20px;"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_account }}</span> 
        </a>
          <ul class="dropdown-menu dropdown-menu-right">
            {% if logged %}
            <li><a href="{{ account }}">{{ text_account }}</a></li>
            <li><a href="{{ order }}">{{ text_order }}</a></li>
            <li class="hidden"><a href="{{ transaction }}">{{ text_transaction }}</a></li>
            <li class="hidden"><a href="{{ download }}">{{ text_download }}</a></li>
            <li><a href="{{ logout }}">{{ text_logout }}</a></li>
            {% else %}
            <li><a href="{{ register }}">{{ text_register }}</a></li>
            <li><a class="loginme" href="{{ login }}">{{ text_login }}</a></li>
            {% endif %}
          </ul>
        </li>
        <li>
            <a 
            href="{{ shopping_cart }}" 
            title="{{ text_shopping_cart }}">
                <div class="item-in-cart">{{ total_item }}</div>
                <i class="fa fa-shopping-cart" style="font-size:20px"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_shopping_cart }}</span></a>
                
            </li>
      </ul>
      </div>
      </div>
  </div>
</header>

<header id="llb-header" class="llb-mobile container visible-xs" style="background-color:#fff" >
    <div id="mobile-menu" style="display:none">
        <div class="text-right toggleMobileMenu"><i class="fa fa-times fa-2x" style="font-size: 26px;"></i></div>
        {{ menu }}
        <hr />
        <ul class="nav navbar-nav">
            {% if logged %}
            <li><a href="{{ account }}">{{ text_account }}</a></li>
            <li><a href="{{ order }}">{{ text_order }}</a></li>
            <li><a href="{{ logout }}">{{ text_logout }}</a></li>
            {% else %}
            <li><a href="{{ register }}">{{ text_register }}</a></li>
            <li><a href="{{ login }}">{{ text_login }}</a></li>
            {% endif %}    
        </ul>
        
        
    </div>
    <div class="row">
        
        <div class="col-xs-7 text-left">
            <a href="{{ home }}">
                  <img src="{{ logo }}" class="aimg-responsive" style="height:56px; padding:5px"/>
                  </a>
        </div>
        
        <div class="col-xs-5 text-right"  style="padding-top:11px; padding-right:29px;">
            <div style="display:inline-block">
                <a href="https://lambatlebohrecs.com/cart" style="line-height:34px; margin-right:2px"><i class="fa fa-search" style="font-size:26px" aria-hidden="true"></i></a>    
            </div>
            <div style="display:inline-block">
                <div class="item-in-cart">{{ total_item }}</div>
                <a href="https://lambatlebohrecs.com/cart" style="line-height:34px; margin-right:10px"><i class="fa fa-shopping-cart" style="font-size:26px" aria-hidden="true"></i></a>    
            </div>
            <div style="display:inline-block">
                <div class="toggleMobileMenu" style="line-height:34px; color:#000"><i class="fa fa-bars" aria-hidden="true" style="font-size:26px"></i></div>      
            </div>
        </div>
        
    </div>
</header>
<div class="llb-content">&nbsp;</div>
<div class="visible-xs" style="padding:8px 15px 0;"><div style="height:40px">{{ search_mobile }}</div></div>

<script>
    $('.toggleMobileMenu').on('click', function(){
        $('#mobile-menu').toggle()
    })
    AOS.init()
</script>